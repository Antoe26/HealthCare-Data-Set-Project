   "source": [
    "# loading data\n",
    "Data=pd.read_csv(\"Cleaned HealthCare Data.csv\", index_col=None)\n",
    "Data.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 38,
   "id": "52b3d12a-f11b-4a28-8152-9a93fd0c1e5d",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "# splitting data into target variable and features\n",
    "y=Data['Had_Insurance']\n",
    "features=['Age','Gender', 'Marital Status', 'Children', 'Employment Status',\n",
    "       'Monthly Household Income','Location']\n",
    "X=Data[features]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
   "id": "fc9cab94-5153-4973-a77c-a785aae842ba",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert age range to midpoint\n",
    "def convert_age_to_midpoint(value):\n",
    "    value = str(value).lower().strip()\n",
    "    if '-' in value:\n",
    "        try:\n",
    "            low, high = map(int, value.replace(',', '').split('-'))\n",
    "            return (low + high) / 2\n",
    "        except:\n",
    "            return np.nan\n",
    "    elif '60+' in value:\n",
    "        return 65\n",
    "    else:\n",
    "        return np.nan\n",
    "\n",
    "# Convert income range to midpoint\n",
    "def convert_income_to_midpoint(value):\n",
    "    value = str(value).lower().strip()\n",
    "    if '-' in value:\n",
    "        try:\n",
    "            low, high = map(int, value.replace(',', '').split('-'))\n",
    "            return (low + high) / 2\n",
    "        except:\n",
    "            return np.nan\n",
    "    elif 'less than' in value:\n",
    "        try:\n",
    "            number = int(value.replace('less than', '').replace(',', '').strip(' -'))\n",
    "            return number / 2\n",
    "        except:\n",
    "            return np.nan\n",
    "    else:\n",
    "        return np.nan"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "id": "3ed60e24-becd-46ea-bc80-63a7c077caee",
   "metadata": {},
   "outputs": [],
   "source": [
    "# applying the functions\n",
    "Data['Age']=Data['Age'].apply(convert_age_to_midpoint)\n",
    "Data['Month Houselhold Income']=Data['Monthly Household Income'].apply(convert_income_to_midpoint)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "id": "28569bc2-331e-4323-b2cb-be78063a074a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# splitting Data\n",
    "X_train,X_val,y_train,y_val=train_test_split(X,y, train_size=0.8, test_size=0.2, random_state=42)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "id": "e817bbca-bb75-42dd-9ccc-4df24f263868",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "# Categorical and numerical data\n",
    "numerical_col=[col for col in X_train.columns if X_train[col].dtype in ['int64','float64']]\n",
    "categorical_col=[col for col in X_train.columns if X_train[col].dtype =='object']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 43,
   "id": "4abe1ea4-3053-4e39-9478-c5bc9f388b3f",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "# Transformers\n",
    "numerical_transformer=StandardScaler()\n",
    "categorical_transformer=Pipeline(steps=[\n",
    "    ('Imputer',SimpleImputer(strategy='most_frequent')),\n",
    "    ('onehot',OneHotEncoder(handle_unknown='ignore'))\n",
    "    \n",
    "])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "80233820-6436-44bc-8a8c-ace9abe9b36c",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "# combining numerical and categorical transformers\n",
    "# PROCESSOR\n",
    "Processor=ColumnTransformer(\n",
    "    transformers=[\n",
    "        ('num', numerical_transformer, numerical_col),\n",
    "        ('cat',categorical_transformer, categorical_col)\n",
    "    ])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "id": "dba46845-6d11-477f-90c2-ea3198396c78",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Defining the model\n",
    "model=RandomForestClassifier(random_state=42,n_estimators=100)\n",
    "\n",
    "my_pipeline=Pipeline(steps=[\n",
    "    ('Processor',Processor),\n",
    "    ('model',model)\n",
    "])"
   ]
  },
